#include<stdio.h>

int main()

{
    int a,b,c;
    for (a=1; a <=10; a++)
    {
            for (b =1 ;b, =12;b++)
            {
             c=a*b;
             printf("4d*%4d =%4d\n",a,b,c);
             
             }
              printf("\n");
              }
              return0
              }   